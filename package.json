    "@angular/platform-server": "~6.0.7",
    "@angular/router": "~6.0.7",
    "core-js": "^2.5.7",
    "global": "^4.3.2",
    "hammerjs": "^2.0.8",
    "ie-shim": "^0.1.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "three": "^0.94.0",
    "zone.js": "~0.8.26"
  },
