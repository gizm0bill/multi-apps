    "karma-mocha-reporter": "^2.2.5",
    "karma-remap-coverage": "^0.1.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "lerna": "^2.11.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "optimize-js-plugin": "0.0.4",
    "parse5": "^5.0.0",
    "protractor": "^5.3.2",
    "pug": "^2.0.3",
