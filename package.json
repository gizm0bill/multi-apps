    "style-loader": "^0.21.0",
    "to-string-loader": "^1.1.5",
    "ts-node": "^7.0.0",
    "tslib": "^1.9.3",
    "tslint": "~5.10.0",
    "tslint-loader": "^3.6.0",
    "typedoc": "^0.11.1",
    "typescript": "2.7",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "url-loader": "^1.0.1",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "~3.1.4",
    "webpack-inline-manifest-plugin": "^4.0.1",
