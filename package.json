    "pug-loader": "^2.4.0",
    "raw-loader": "0.5.1",
    "rimraf": "~2.6.1",
    "rxjs-compat": "^6.2.2",
    "rxjs-tslint": "^0.1.5",
    "sass-loader": "^7.0.3",
